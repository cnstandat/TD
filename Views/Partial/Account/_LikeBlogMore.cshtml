@model IEnumerable<TD.Models.Views.UserBlogView>

@foreach (var item in Model)
{
    <div class="table-row">
        <span class="table-cell">
            @item.FullName
        </span>
        <span class="table-cell">
            @item.LastModifyUtc
        </span>
        <span class="table-cell">
            @item.CommentCount
        </span>
        <span class="table-cell">
            @item.Viewed
        </span>
    </div>
}
@if (ViewBag.Page != null)
{
    <div class="text-center">
        <a data-src="/account/_likeblogmore/@ViewBag.Page" class="btn btn-info load-on-click">
            <i class="fa fa-menu"></i> Xem thêm
        </a>
    </div>
}












