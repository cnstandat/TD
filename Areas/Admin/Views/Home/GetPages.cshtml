@model IEnumerable<TD.Models.Config>
<h4 class="form-title">Danh sách trang</h4>
<div class="table">
    <div class="table-row table-row-header">
        <span class="table-cell">
            Tiêu đề
            <a href="/admin/createpage" class="btn left" title="Thêm mới">
                Thêm mới
            </a>
        </span>
        
    </div>

    @foreach (var item in Model)
    {
        <div class="table-row">
            <span class="table-cell">
                <a href="/admin/editpage/@item.Id" title="Click vào đây để xem chi tiết">
                  <i class="fa fa-edit"></i>   @item.Name
                </a>
            </span>
            
        </div>

    }
</div>