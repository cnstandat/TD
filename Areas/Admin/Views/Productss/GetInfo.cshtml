@model TD.Models.Views.AppInfos
<h4 class="form-title">Thông số sản phẩm</h4>
<div class="table">
    <div class="table-row table-row-header">
        <span class="table-cell">
            <a href="/admin/products/info">
                Thông số sản phẩm
            </a>
            <a href="/admin/products/createInfo/@Model.AppId" class="btn btn-mn left" title="Thêm mới">
                Thêm mới
            </a>
        </span>
        <span class="table-cell">
            Giá trị
        </span>
    </div>
  
    @foreach (var item in Model.Infos)
    {<div class="table-row">
        <span class="table-cell">
            <a href="/admin/products/editAppInfo?AppInfoId=@item.AppInfoId&AppId=@item.AppId" title="Click vào đây để xem chi tiết">
                @item.InfoName
            </a>
        </span>
        <span class="table-cell">
            @item.Value
        </span>
    </div>

    }
</div>