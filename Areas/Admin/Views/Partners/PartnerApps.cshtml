@model IEnumerable<TD.Models.Views.PartnerAppView>

<div class="table">
    <div class="table-row table-row-header">
        <span class="table-cell">
            Ứng dụng
        </span>
        <span class="table-cell">
            Ngày sử dụng
        </span>
        <span class="table-cell">
            Ngày hết hạn
        </span>
        <span class="table-cell">
            Bản quyền
        </span>
    </div>
    @if (Model != null && Model.Any())
    {
        foreach (var item in Model)
        {
            <div class="table-row">
                <span class="table-cell">
                    <span>@item.AppName</span>
                </span>
                <span class="table-cell">
                    @item.Start.ToString("dd/MM/yy")
                </span>
                <span class="table-cell">
                    @item.Expires
                </span>
                <span class="table-cell">
                    @item.LicenseType.GetEnumDescription()
                </span>
            </div>

        }
    }
    else
    {
        <div class="table-row">
            <span class="table-cell">Không có dữ liệu</span>
        </div>
    }

</div>